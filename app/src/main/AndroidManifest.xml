<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.distributoreautomatico">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_vendingmachine"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_vendingmachine"
        android:supportsRtl="true"
        android:theme="@style/Theme.DistributoreAutomatico">
        <activity android:name=".GuestMenuActivity"></activity>
        <activity android:name=".AdminMenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>