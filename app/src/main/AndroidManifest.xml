<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.distributoreautomatico">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_vendingmachine"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_vendingmachine"
        android:supportsRtl="true"
        android:theme="@style/Theme.DistributoreAutomatico">
        <activity
            android:name=".GuestMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>